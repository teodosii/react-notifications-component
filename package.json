		"eslint-plugin-react": "^7.10.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^2.0.0",
		"font-awesome": "^4.7.0",
		"gh-pages": "^2.0.1",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.1",
		"node-sass": "^4.11.0",
		"object-assign": "^4.1.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.18.2",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-github-button": "^0.1.11",
