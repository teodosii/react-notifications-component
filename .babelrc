{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "tests": "./tests",
          "src": "./src",
          "samples": "./samples"
        }
      }
    ]
  ]
}